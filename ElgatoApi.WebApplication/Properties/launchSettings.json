{
  "$schema": "https://json.schemastore.org/launchsettings.json",
  "profiles": {
    "ElgatoApi.WebApplication": {
      "commandName": "Project",
      "dotnetRunMessages": true,
      "launchBrowser": true,
      "launchUrl": "lights",
      "applicationUrl": "https://elgatoapi:49587",
      "environmentVariables": {
        "ASPNETCORE_ENVIRONMENT": "Development",
        "ASPNETCORE_Kestrel__Certificates__Default__Path": "%userprofile%\\.aspnet\\https\\elgatoapi.crt",
        "ASPNETCORE_Kestrel__Certificates__Default__KeyPath": "%userprofile%\\.aspnet\\https\\elgatoapi.key"
      }
    }
  }
}
